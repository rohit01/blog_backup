---
layout: blank
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Simplicity - @rohit01</title>
  <link href="http://www.rohit.io/rss.xml" rel="self"/>
  <link href="http://www.rohit.io/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://www.rohit.io/</id>
  <author>
    <name>{{ site.author_name }}</name>
    <email>hello@rohit.io</email>
  </author>
  {% for post in site.posts limit:10 %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://www.rohit.io{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://www.rohit.io{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
    <author>
      <name>{{ site.author_name }}</name>
      <uri>http://www.rohit.io/about/</uri>
    </author>
  </entry>
  {% endfor %}
</feed>
